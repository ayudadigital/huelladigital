{
  "containerDefinitions": [
    {
      "name": "dev-backend-container",
      "image": "283609209591.dkr.ecr.us-east-1.amazonaws.com/huellapositiva/backend:1.13",
      "cpu": 1024,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "dev-huellapositiva-backend-lg",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-dev-huellapositiva-backend"
        }
      },
      "essential": true,
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "BASE_URI",
          "value": "https://dev.huellpositiva.ibai.eus"
        },
        {
          "name": "PROFILE",
          "value": "dev"
        },
        {
          "name": "SERVICE_NAME",
          "value": "backend"
        }
      ],
      "mountPoints": [],
      "volumesFrom": []
    }
  ],
  "family": "backend-task",
  "taskRoleArn": "arn:aws:iam::283609209591:role/huellapositiva-ecs-service-backend",
  "executionRoleArn": "arn:aws:iam::283609209591:role/huellapositiva-ecs-service-backend-execution",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "1024",
  "memory": "512"
}
